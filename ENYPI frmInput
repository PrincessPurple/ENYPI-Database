'Main Input GUI

Private Sub btnCaseInfo_Click()

If txtCase.Value = "" Then
MsgBox "Please Enter A Case Name"
Else
    ActiveSheet.PageSetup.CenterHeader = txtCase
    txtCaseName.Visible = True
    txtCaseName.Text = txtCase
End If

End Sub

Private Sub btnClearPatient_Click()

lstCode.Value = ""
txtPCRF.Text = ""
txtTime2.Text = ""
txtDate2.Text = ""
txtLocation.Text = ""
txtAge.Text = ""
txtSex.Text = ""
txtIncident.Text = ""
txtTreatment.Text = ""
txtPublic.Text

End Sub

Private Sub btnClose_Click()

frmClose.Show

End Sub


Private Sub btnClearNotes_Click()

txtTime1.Text = ""
txtDate1.Text = ""
txtNotes.Text = ""

End Sub

Private Sub btnCloseGUI_Click()

End

End Sub

Private Sub btnInsert_Click()

Dim LastRow As Long
i = Sheets("sheet3").Cells(1, 1)
i = i + 1
Sheets("Notes").Select


'Seeing IF all the values have been entered
If txtTime1.Value = "" Then
txtTime1.Value = "Not Recorded"
ElseIf txtDate1.Value = "" Then
txtDate1.Value = "Not Recorded"
ElseIf txtNotes.Value = "" Then
MsgBox "Please Type In Notes", vbExclamation, "Warning"
Else
'Find the last used row in Colum A & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "A").End(xlUp).Row
    End With
    Cells(LastRow + 1, 1) = txtTime1.Value
'Find the last used row in Colum B & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "B").End(xlUp).Row
    End With
    Cells(LastRow + 1, 2) = txtDate1.Value
'Find the last used row in Colum C & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "C").End(xlUp).Row
    End With
    Cells(LastRow + 1, 3) = txtNotes.Value
'Clears text from all data entry points
    txtTime1.Text = ""
    txtNotes.Text = ""

MsgBox "Information Sucsesfully Submitted", vbInformation, "Information"
End If
Sheets("sheet3").Cells(1, 1) = i

End Sub

Private Sub btnLogIn_Click()

frmLogIn.Show

End Sub

Private Sub btnMember_Click()

Dim LastRow As Long
i = Sheets("sheet3").Cells(1, 1)
i = i + 1
Sheets("Onsite Members").Select


'Seeing IF all the values have been entered
If txtName.Value = "" Then
MsgBox "Please Insert A Name", vbExclamation, "Warning"
ElseIf txtCall.Value = "" Then
txtCall.Value = "N/A"
ElseIf txtRole.Value = "" Then
MsgBox "Please State A Role"
ElseIf txtInfo.Value = "" Then
txtInfo.Value = "None Recorded"
Else
'Find the last used row in Colum A & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "A").End(xlUp).Row
    End With
    Cells(LastRow + 1, 1) = txtName.Value
'Find the last used row in Colum B & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "B").End(xlUp).Row
    End With
    Cells(LastRow + 1, 2) = txtCall.Value
'Find the last used row in Colum C & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "C").End(xlUp).Row
    End With
    Cells(LastRow + 1, 3) = txtRole.Value
'Find the last used row in Colum D & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "D").End(xlUp).Row
    End With
    Cells(LastRow + 1, 4) = txtInfo.Value
'Clears text from all data entry points
    txtName.Text = ""
    txtCall.Text = ""
    txtRole.Text = ""
    txtInfo.Text = ""
    
MsgBox "Information Sucsesfully Submitted", vbInformation, "Information"
End If
Sheets("sheet3").Cells(1, 1) = i

End Sub

Private Sub btnPatient_Click()

Dim LastRow As Long
i = Sheets("sheet3").Cells(1, 1)
i = i + 1
Sheets("Casulty Info").Select

'Seeing IF all the values have been entered
If lstCode.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtPCRF.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtTime2.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtDate2.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtLocation.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtAge.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtSex.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtIncident.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtTreatment.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf lstPublic.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf lstStaff.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf lstRIDDOR.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf lstDisposal.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtFirstAider.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtCall3.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
ElseIf txtNumber.Value = "" Then
MsgBox "Please Enter ALL Values", vbCritical, "Warning"
Else
'Find the last used row in Colum A & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "A").End(xlUp).Row
    End With
    Cells(LastRow + 1, 1) = lstCode.Value
'Find the last used row in Colum B & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "B").End(xlUp).Row
    End With
    Cells(LastRow + 1, 2) = txtPCRF.Value
'Find the last used row in Colum C & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "C").End(xlUp).Row
    End With
    Cells(LastRow + 1, 3) = txtTime2.Value
'Find the last used row in Colum D & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "D").End(xlUp).Row
    End With
    Cells(LastRow + 1, 4) = txtDate2.Value
'Find the last used row in Colum E & insert text from TextBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "E").End(xlUp).Row
    End With
    Cells(LastRow + 1, 5) = txtLocation.Value
'Find the last used row in Colum F & insert text from TextBox2
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "F").End(xlUp).Row
    End With
    Cells(LastRow + 1, 6) = txtAge.Value
'Find the last used row in Colum G & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "G").End(xlUp).Row
    End With
    Cells(LastRow + 1, 7) = txtSex.Value
    'Find the last used row in Colum H & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "H").End(xlUp).Row
    End With
    Cells(LastRow + 1, 8) = txtIncident.Value
'Find the last used row in Colum I & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "I").End(xlUp).Row
    End With
    Cells(LastRow + 1, 9) = txtTreatment.Value
'Find the last used row in Colum J & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "J").End(xlUp).Row
    End With
    Cells(LastRow + 1, 10) = lstPublic.Value
'Find the last used row in Colum K & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "K").End(xlUp).Row
    End With
    Cells(LastRow + 1, 11) = lstStaff.Value
'Find the last used row in Colum L & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "L").End(xlUp).Row
    End With
    Cells(LastRow + 1, 12) = lstRIDDOR.Value
'Find the last used row in Colum M & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "M").End(xlUp).Row
    End With
    Cells(LastRow + 1, 13) = lstDisposal.Value
'Find the last used row in Colum N & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "N").End(xlUp).Row
    End With
    Cells(LastRow + 1, 14) = txtFirstAider.Value
'Find the last used row in Colum O & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "O").End(xlUp).Row
    End With
    Cells(LastRow + 1, 15) = txtCall3.Value
'Find the last used row in Colum P & inserts data from ComboBox1
    With ActiveSheet
        LastRow = .Cells(.Rows.Count, "P").End(xlUp).Row
    End With
    Cells(LastRow + 1, 16) = txtNumber.Value
    
'Clears text from all data entry points
    lstCode.Value = ""
    txtPCRF.Text = ""
    txtTime2.Text = ""
    txtDate2.Text = ""
    txtLocation.Text = ""
    txtAge.Text = ""
    txtSex.Text = ""
    txtIncident.Text = ""
    txtTreatment.Text = ""
    lstPublic.Value = ""
    lstStaff.Value = ""
    lstRIDDOR.Value = ""
    lstDisposal.Value = ""
    txtFirstAider.Text = ""
    txtCall3.Text = ""
    txtNumber.Text = ""

    MsgBox "Information Sucsesfully Submitted", vbInformation, "Information"
    End If
Sheets("sheet3").Cells(1, 1) = i
End Sub

